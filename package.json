{
  "name": "cyber-container-platform",
  "version": "1.0.0",
  "description": "A modern, self-hosted container management platform",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && go run main.go",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["docker", "containers", "self-hosted", "management", "orchestration"],
  "author": "Cyber Container Platform",
  "license": "MIT"
}
